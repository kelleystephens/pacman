doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    title Pacman
    link(rel='stylesheet', href='./css/app.css')
  body
    h1 Pac-man
    button#start Start!
    table#board
      tbody
        -for(var y = 0; y < 3; y++)
          tr
            -for(var x = 0; x < 2; x++)
              td(data-x = x, data-y = y)

    script(src='./js/vendor/jquery-2.1.0.js')
    script(src='./js/source/main.js')
